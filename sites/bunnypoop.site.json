{
  "name": "Bunny Poop",
  "version": "0.2.0-alpha.3",
  "git_repo": "git@github.com:jkenlooper/bunnypoop.git",
  "contact": {
    "owner": "jake@weboftomorrow.com",
    "technical": "jake@weboftomorrow.com"
  },
  "domain_list": [
    "bunnypoop.massive.xyz"
  ],
  "chill_env": [
    {
      "name": "CHILL_HOST",
      "value": "localhost"
    },
    {
      "name": "CHILL_PORT",
      "value": "5001"
    },
    {
      "name": "CHILL_MEDIA_PATH",
      "value": "/media/"
    },
    {
      "name": "CHILL_THEME_STATIC_PATH",
      "value": "/theme/$version/"
    },
    {
      "name": "CHILL_DESIGN_TOKENS_HOST",
      "value": "/design-tokens/$version/"
    }
  ],
  "env": [
    {
      "name": "BUNNYPOOP_SERVER_NAME",
      "value": "$server_name"
    },
    {
      "name": "BUNNYPOOP_VERSION",
      "value": "$version"
    },
    {
      "name": "BUNNYPOOP_CHILL",
      "value": "http://localhost:5001/"
    },
    {
     "name": "BUNNYPOOP_IMMUTABLE__DESIGN_TOKENS",
     "value": "$S3_ENDPOINT_URL/$IMMUTABLE_BUCKET_NAME/$slugname/$version/design-tokens/"
    },
    {
     "name": "BUNNYPOOP_IMMUTABLE__CLIENT_SIDE_PUBLIC",
     "value": "$S3_ENDPOINT_URL/$IMMUTABLE_BUCKET_NAME/$slugname/$version/client-side-public/"
    }
  ]
}
